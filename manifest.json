{
    "manifest_version": 2,
    "name":"Key logger",
    "version":"1.0",
    "description":"Keylogger extension",
    "icons":{
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "browser_action":{
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "permissions":[
        "storage",
        "http://*.google.com/"
    ]
}